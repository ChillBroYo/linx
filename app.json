{
  "expo": {
    "name": "Linx",
    "slug": "Lets-get-linx",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.11",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "notification": {
        "icon": "./assets/images/notification_icon.png",
        "iosDisplayInForeground": true
    },
    "splash": {
      "image": "./assets/images/cover_image.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.linx.linx-app",
      "infoPlist": {
        "NSCameraUsageDescription": "Use your camera to take a profile picture so your new friends can see who you are."
      }
    },
    "android": {
      "package": "com.linx.linx",
      "versionCode": 7,
      "googleServicesFile": "./google-services.json"
    },
    "description": ""
  }
}
